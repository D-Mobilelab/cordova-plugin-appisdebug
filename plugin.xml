<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-androidisdebug" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>AndroidIsDebug</name>
	<description>Android debug status for Cordova Apps</description>
	<license>Apache 2.0</license>

	<js-module name="AndroidIsDebug" src="www/AndroidIsDebug.js">
		<clobbers target="cordova.plugins.AndroidIsDebug" />
	</js-module>

	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="AndroidIsDebug">
				<param name="android-package" value="com.d-mobilelab.AndroidIsDebug" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml" />

		<source-file src="src/android/AndroidIsDebug.java" target-dir="src/com/d-mobilelab/AndroidIsDebug" />
	</platform>
</plugin>